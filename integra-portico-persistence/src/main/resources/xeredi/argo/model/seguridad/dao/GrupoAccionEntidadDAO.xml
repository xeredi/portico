<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="xeredi.argo.model.seguridad.dao.GrupoAccionEntidadDAO">
    <resultMap type="GrupoAccionEntidadVO" id="ResultMap">
        <id column="grae_grpo_pk" />
        <id column="grae_acen_pk" />

        <result column="grae_grpo_pk" property="grpoId" />
        <result column="grae_acen_pk" property="acenId" />
    </resultMap>

    <sql id="SelectPrefix">
        SELECT grae_grpo_pk, grae_acen_pk
        FROM tbl_grupo_accion_entidad_grae
    </sql>

    <sql id="SelectWhere">
        <where>
            <if test="grpoId != null">
                AND grae_grpo_pk = #{grpoId}
            </if>
            <if test="acenId != null">
                AND grae_acen_pk = #{acenId}
            </if>
        </where>
    </sql>

    <select id="exists" parameterType="GrupoAccionEntidadVO" resultType="Boolean">
        <![CDATA[
        SELECT COUNT(1)
        FROM tbl_grupo_accion_entidad_grae
        WHERE
            grae_grpo_pk = #{grpoId}
            AND grae_acen_pk = #{acenId}
        ]]>
    </select>

    <select id="selectObject" parameterType="GrupoAccionEntidadCriterioVO" resultMap="ResultMap">
        <include refid="SelectPrefix" />
        <include refid="SelectWhere" />
    </select>

    <select id="selectList" parameterType="GrupoAccionEntidadCriterioVO" resultMap="ResultMap">
        <include refid="SelectPrefix" />
        <include refid="SelectWhere" />
    </select>

    <insert id="insert" parameterType="GrupoAccionEntidadVO">
        <![CDATA[
        INSERT INTO tbl_grupo_accion_entidad_grae (grae_grpo_pk, grae_acen_pk)
        VALUES (#{grpoId}, #{acenId})
        ]]>
    </insert>

    <delete id="delete" parameterType="GrupoAccionEntidadVO">
        <![CDATA[
        DELETE FROM tbl_grupo_accion_entidad_grae
        WHERE grae_grpo_pk = #{grpoId}
            AND grae_acen_pk = #{acenId}
        ]]>
    </delete>

    <delete id="deleteList" parameterType="GrupoAccionCriterioVO">
        DELETE FROM tbl_grupo_accion_entidad_grae
        <include refid="SelectWhere" />
    </delete>
</mapper>
