<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="xeredi.integra.model.proceso.dao.ProcesoArchivoDAO">
	<resultMap type="ProcesoArchivoVO" id="ResultMap">
		<id column="prar_prbt_pk" />
		<id column="prar_arch_pk" />

		<result column="prar_prbt_pk" property="prbtId" />
		<result column="prar_arch_pk" property="archId" />
	</resultMap>

	<sql id="SelectPrefix">
    <![CDATA[
    SELECT
        prar_prbt_pk
        , prar_arch_pk
    FROM
        tbl_proceso_archivo_prar
    ]]>
	</sql>

	<sql id="SelectWhere">
		<if test="id != null">
			AND prar_prbt_pk = #{id}
		</if>
	</sql>

	<select id="selectList" parameterType="ProcesoCriterioVO" resultMap="ResultMap">
		<include refid="SelectPrefix" />
		<include refid="SelectWhere" />
	</select>

	<insert id="insert" parameterType="ProcesoArchivoVO">
        <![CDATA[
        INSERT INTO tbl_proceso_archivo_prar (prar_prbt_pk, prar_arch_pk)
        VALUES (#{prbtId}, #{archId})
        ]]>
	</insert>

	<delete id="deleteList" parameterType="ProcesoCriterioVO">
        DELETE FROM tbl_proceso_archivo_prar
		<include refid="SelectWhere" />
	</delete>
</mapper>
