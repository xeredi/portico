<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="xeredi.integra.model.servicio.dao.SubservicioTramiteDatoDAO">
	<resultMap type="ItemTramiteDatoVO" id="ResultMap">
		<id column="sstd_sstr_pk" />
		<id column="sstd_tpdt_pk" />

		<result column="sstd_sstr_pk" property="ittrId" />
		<result column="sstd_tpdt_pk" property="tpdtId" />

		<result column="sstd_o_nentero" property="onentero" />
		<result column="sstd_o_ndecimal" property="ondecimal" />
		<result column="sstd_o_cadena" property="ocadena" />
		<result column="sstd_o_fecha" property="ofecha" />

		<result column="sstd_d_nentero" property="dnentero" />
		<result column="sstd_d_ndecimal" property="dndecimal" />
		<result column="sstd_d_cadena" property="dcadena" />
		<result column="sstd_d_fecha" property="dfecha" />

		<association property="oprmt" javaType="ParametroVO">
			<result column="sstd_o_prmt_pk" property="id" />
		</association>

		<association property="dprmt" javaType="ParametroVO">
			<result column="sstd_d_prmt_pk" property="id" />
		</association>

		<association property="osrvc" javaType="ServicioVO">
			<result column="sstd_o_srvc_pk" property="id" />
		</association>

		<association property="dsrvc" javaType="ServicioVO">
			<result column="sstd_d_srvc_pk" property="id" />
		</association>
	</resultMap>

	<sql id="SelectPrefix">
    <![CDATA[
		SELECT
			sstd_sstr_pk, sstd_tpdt_pk
			, sstd_o_nentero, sstd_o_ndecimal, sstd_o_cadena, sstd_o_fecha, sstd_o_prmt_pk, sstd_o_srvc_pk
			, sstd_d_nentero, sstd_d_ndecimal, sstd_d_cadena, sstd_d_fecha, sstd_d_prmt_pk, sstd_d_srvc_pk
		FROM
			tbl_subservicio_trmt_dato_sstd
	]]>
	</sql>

	<sql id="SelectWhere">
		<where>
			<if test="id != null">
				AND sstd_sstr_pk = #{id}
			</if>
		</where>
	</sql>

	<select id="selectList" parameterType="SubservicioTramiteCriterioVO" resultMap="ResultMap">
		<include refid="SelectPrefix" />
		<include refid="SelectWhere" />
	</select>

	<insert id="insert" parameterType="ItemTramiteDatoVO">
        <![CDATA[
        INSERT INTO tbl_subservicio_trmt_dato_sstd (
        	sstd_sstr_pk, sstd_tpdt_pk
			, sstd_o_nentero, sstd_o_ndecimal, sstd_o_cadena, sstd_o_fecha, sstd_o_prmt_pk, sstd_o_srvc_pk
			, sstd_d_nentero, sstd_d_ndecimal, sstd_d_cadena, sstd_d_fecha, sstd_d_prmt_pk, sstd_d_srvc_pk)
        VALUES (
        	#{ittrId}, #{tpdtId}
        	, #{onentero}, #{ondecimal}, #{ocadena}, #{ofecha}, #{oprmt.id}, #{osrvc.id}
        	, #{dnentero}, #{dndecimal}, #{dcadena}, #{dfecha}, #{dprmt.id}, #{dsrvc.id}
        )
        ]]>
	</insert>
</mapper>
