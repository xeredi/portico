<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="xeredi.integra.model.dao.servicio.SubservicioSubservicioDAO">
    <resultMap type="SubservicioSubservicioVO" id="ResultMap">
        <id column="ssss_ssrvp_pk" property="ssrvPadreId" />
        <id column="ssss_ssrvh_pk" property="ssrvHijoId" />
    </resultMap>

    <select id="exists" parameterType="SubservicioSubservicioVO" resultType="boolean">
        <![CDATA[
        SELECT COUNT(1)
        FROM tbl_subserv_subserv_ssss
        WHERE ssss_ssrvp_pk = #{ssrvPadreId}
            AND ssss_ssrvh_pk = #{ssrvHijoId}
        ]]>
    </select>

    <insert id="insert" parameterType="SubservicioSubservicioVO">
        <![CDATA[
        INSERT INTO tbl_subserv_subserv_ssss (
            ssss_ssrvp_pk, ssss_ssrvh_pk
        ) VALUES (
            #{ssrvPadreId}, #{ssrvHijoId}
        )
        ]]>
    </insert>
</mapper>
