<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="xeredi.integra.model.dao.facturacion.FacturaDetalleDAO">
    <resultMap type="FacturaDetalleVO" id="ResultMap">
        <id column="fctd_pk" />

        <result column="fctd_pk" property="id" />
        <result column="fctd_fctr_pk" property="fctrId" />
        <result column="fctd_fctl_pk" property="fctlId" />

        <result column="fctd_importe_base" property="importeBase" />
        <result column="fctd_importe" property="importe" />

        <result column="fctl_cuant1" property="cuant1" />
        <result column="fctl_cuant2" property="cuant2" />
        <result column="fctl_cuant3" property="cuant3" />
        <result column="fctl_cuant4" property="cuant4" />
        <result column="fctl_cuant5" property="cuant5" />
        <result column="fctl_cuant6" property="cuant6" />

        <result column="fctl_info1" property="info1" />
        <result column="fctl_info2" property="info2" />
        <result column="fctl_info3" property="info3" />
        <result column="fctl_info4" property="info4" />
        <result column="fctl_info5" property="info5" />
        <result column="fctl_info6" property="info6" />

        <association property="ssrv" resultMap="xeredi.integra.model.dao.maestro.ParametroDAO.ResultMap" />
    </resultMap>

    <insert id="insert" parameterType="FacturaDetalleVO">
    <![CDATA[
        INSERT INTO tbl_factura_det_fctd (
            fctd_pk, fctd_fctr_pk, fctd_fctl_pk, fctd_importe_base, fctd_importe, fctd_ssrv_pk
            , fctd_cuant1, fctd_cuant2, fctd_cuant3, fctd_cuant4, fctd_cuant5, fctd_cuant6
            , fctd_info1, fctd_info2, fctd_info3, fctd_info4, fctd_info5, fctd_info6
        ) VALUES (
            #{id}, #{fctrId}, #{fctlId}, #{importeBase}, #{importe}, #{ssrv.id}
            , #{cuant1}, #{cuant2}, #{cuant3}, #{cuant4}, #{cuant5}, #{cuant6}
            , #{info1}, #{info2}, #{info3}, #{info4}, #{info5}, #{info6}
        )
    ]]>
    </insert>
</mapper>
